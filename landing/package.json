{
  "name": "@devhub/landing",
  "version": "0.101.0",
  "private": false,
  "main": "dist/index.js",
  "scripts": {
    "clean": "shx rm -f *.tsbuildinfo && shx rm -rf dist && mkdirp dist",
    "compile": "cd .",
    "deploy": "yarn compile && yarn now",
    "export": "yarn run-scripts && next build && next export",
    "format": "prettier --write '{.,src/**}/*.{js,jsx,ts,tsx}'",
    "lint": "tslint -p .",
    "now": "now",
    "postinstall": "yarn run-scripts",
    "prepare": "cd .",
    "run-scripts": "TS_NODE_COMPILER_OPTIONS='{\"module\":\"commonjs\"}' ts-node ./src/scripts/refresh-cached-public-plans",
    "start": "next -p 3001"
  },
  "dependencies": {
    "@devhub/core": "npm:@brunolemos/devhub-core@0.101.0",
    "@zeit/next-css": "1.0.1",
    "autoprefixer": "9.6.4",
    "classnames": "2.2.6",
    "isomorphic-unfetch": "3.0.0",
    "lodash": "4.17.15",
    "next": "10.0.1",
    "next-transpile-modules": "4.1.0",
    "qs": "6.9.1",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-stripe-elements": "5.0.1",
    "tailwindcss": "1.1.2"
  },
  "devDependencies": {
    "@types/classnames": "2.2.9",
    "@types/lodash": "4.14.149",
    "@types/node": "14.0.5",
    "@types/qs": "6.9.0",
    "@types/react": "16.9.35",
    "@types/react-dom": "16.9.8",
    "@types/react-stripe-elements": "1.3.5",
    "@types/stripe-v3": "3.1.9",
    "@types/styled-jsx": "2.2.8",
    "mkdirp": "0.5.1",
    "node-fetch": "2.6.0",
    "now": "20.1.2",
    "prettier": "2.0.5",
    "ts-node": "8.6.2",
    "tslint": "6.1.3",
    "tslint-config-airbnb": "5.11.2",
    "tslint-config-prettier": "1.18.0",
    "tslint-react": "5.0.0",
    "typescript": "4.0.5"
  },
  "engines": {
    "node": ">=12"
  }
}