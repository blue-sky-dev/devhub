{
  "name": "devhub",
  "alias": "devhubapp.com",
  "version": 2,
  "builds": [
    { "src": "packages/web/dist/**", "use": "@now/static" }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/packages/web/dist/$1"
    }
  ],
  "regions": ["gru1"],
  "github": {
    "autoAlias": false
  }
}
