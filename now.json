{
  "name": "devhub",
  "alias": "devhubapp.com",
  "builds": [
    { "src": "packages/web/dist/**", "use": "@now/static" },
    { "src": "packages/server/index.js", "use": "@now/node" }
  ],
  "routes": [
    { "src": "/api", "dest": "/packages/server/" },
    { "src": "/(.*)", "dest": "/packages/web/dist/$1" }
  ],
  "regions": ["gru1"],
  "version": 2
}
