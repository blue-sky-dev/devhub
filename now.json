{
  "public": true,
  "name": "devhub",
  "alias": "devhubapp.com",
  "version": 2,
  "builds": [
    { "src": "packages/web/dist/**", "use": "@now/static" },
    { "src": "next.config.js", "use": "@now/next" }
  ],
  "routes": [
    {
      "src": "/_next/(.*)",
      "dest": "/_next/$1"
    },
    {
      "src": "/(.*)\\.(.*)",
      "dest": "/packages/web/dist/$1.$2"
    }
  ],
  "regions": ["all"],
  "github": {
    "enabled": false,
    "autoAlias": false
  }
}
