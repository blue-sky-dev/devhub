    "@types/fbemitter": "2.0.32",
    "@types/lodash": "4.14.123",
    "mkdirp": "0.5.1",
    "prettier": "1.16.4",
    "tslint": "5.14.0",
    "tslint-config-airbnb": "5.11.1",
