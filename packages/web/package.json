{
  "name": "@devhub/web",
  "version": "0.92.0",
  "private": false,
  "homepage": "./",
  "scripts": {
    "build": "yarn compile && react-app-rewired build && shx rm -rf dist && mv build dist",
    "clean": "shx rm -rf dist/*",
    "compile": "tsc -b",
    "deploy": "yarn build && cd ../../ && now --target production && cd -",
    "deploy:beta": "yarn build && cd ../../ && now --target staging && now alias beta.devhubapp.com && cd -",
    "eject": "react-app-rewired eject",
    "format": "prettier --write '{.,src/**}/*.{js,jsx,ts,tsx}'",
    "lint": "tslint -p .",
    "prepare": "cd .. && yarn patch-package",
    "serve": "serve dist",
    "start": "react-app-rewired start",
    "test": "react-app-rewired test"
  },
  "dependencies": {
    "@devhub/components": "0.92.0",
    "react": "16.8.6",
    "react-app-polyfill": "1.0.1",
    "react-art": "16.8.6",
    "react-dom": "16.8.6",
    "react-native-web": "0.11.1",
    "react-scripts": "3.0.1",
    "resize-observer-polyfill": "1.5.1",
    "smoothscroll-polyfill": "0.4.3"
  },
  "devDependencies": {
    "@types/jest": "24.0.11",
    "@types/node": "11.11.4",
    "@types/react": "16.8.15",
    "@types/react-dom": "16.8.4",
    "babel-plugin-react-native-web": "0.11.1",
    "now": "*",
    "postinstall-prepare": "1.0.1",
    "prettier": "1.16.4",
    "react-app-rewired": "2.1.1",
    "serve": "10.1.2",
    "tslint": "5.14.0",
    "tslint-config-airbnb": "5.11.1",
    "tslint-config-prettier": "1.18.0",
    "tslint-react": "3.6.0",
    "typescript": "3.5.1",
    "webpack-bundle-analyzer": "3.1.0"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    "last 1 electron version",
    "last 1 chrome version",
    "last 1 firefox version",
    "last 1 safari version"
  ]
}
