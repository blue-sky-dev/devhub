{
  "name": "@devhub/web",
  "version": "0.99.1",
  "private": false,
  "homepage": "./",
  "scripts": {
    "build": "yarn compile && react-app-rewired build && shx rm -rf dist && mv build dist",
    "clean": "shx rm -rf dist/*",
    "compile": "tsc -b --incremental",
    "deploy": "yarn build && cd ../../ && now --local-config ./now.app.json --prod && cd -",
    "deploy:beta": "yarn build && cd ../../ && now --local-config ./now.beta.json --prod && cd -",
    "eject": "react-app-rewired eject",
    "format": "prettier --write '{.,src/**}/*.{js,jsx,ts,tsx}'",
    "lint": "tslint -p .",
    "prepare": "cd .. && yarn patch-package",
    "serve": "serve dist",
    "start": "react-app-rewired start",
    "test": "react-app-rewired test"
  },
  "dependencies": {
    "@devhub/components": "0.99.1",
    "localforage": "1.7.3",
    "react": "16.12.0",
    "react-app-polyfill": "1.0.4",
    "react-art": "16.12.0",
    "react-dom": "16.12.0",
    "react-native-web": "0.12.0-rc.1",
    "react-scripts": "3.2.0",
    "react-stripe-elements": "5.0.1",
    "resize-observer-polyfill": "1.5.1",
    "smoothscroll-polyfill": "0.4.4"
  },
  "devDependencies": {
    "@types/jest": "24.0.18",
    "@types/node": "12.7.11",
    "@types/react": "16.9.15",
    "@types/react-dom": "16.9.4",
    "@types/react-stripe-elements": "1.3.5",
    "@types/stripe-v3": "3.1.9",
    "babel-plugin-react-native-web": "0.12.0-rc.1",
    "now": "16.7.1",
    "postinstall-prepare": "1.0.1",
    "prettier": "1.18.2",
    "react-app-rewired": "2.1.3",
    "serve": "11.2.0",
    "tslint": "5.20.1",
    "tslint-config-airbnb": "5.11.2",
    "tslint-config-prettier": "1.18.0",
    "tslint-react": "4.1.0",
    "typescript": "3.7.2",
    "webpack-bundle-analyzer": "3.5.2"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    "last 1 electron version",
    "last 1 chrome version",
    "last 1 firefox version",
    "last 1 safari version"
  ]
}