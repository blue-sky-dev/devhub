{
  "name": "@devhub/web",
  "version": "0.101.0",
  "private": false,
  "homepage": "./",
  "scripts": {
    "build": "yarn compile && react-app-rewired build && shx rm -rf dist && mv build dist",
    "clean": "shx rm -rf dist/*",
    "compile": "tsc -b",
    "deploy": "yarn build && cd ../../ && now --local-config ./now.app.json --prod && cd -",
    "deploy:beta": "yarn build && cd ../../ && now --local-config ./now.beta.json --prod && cd -",
    "eject": "react-app-rewired eject",
    "format": "prettier --write '{.,src}/*.{js,jsx,ts,tsx}'",
    "lint": "eslint src --no-error-on-unmatched-pattern",
    "prepare": "cd .. && yarn patch-package",
    "serve": "serve dist",
    "start": "react-app-rewired start",
    "test": "react-app-rewired test"
  },
  "dependencies": {
    "@devhub/components": "0.101.0",
    "localforage": "1.7.3",
    "react": "17.0.1",
    "react-app-polyfill": "2.0.0",
    "react-art": "17.0.1",
    "react-dom": "17.0.1",
    "react-native-web": "0.14.9",
    "react-scripts": "4.0.1",
    "react-stripe-elements": "5.0.1",
    "resize-observer-polyfill": "1.5.1",
    "smoothscroll-polyfill": "0.4.4"
  },
  "devDependencies": {
    "@types/jest": "26.0.16",
    "@types/node": "14.0.5",
    "@types/react": "17.0.0",
    "@types/react-dom": "17.0.0",
    "@types/react-stripe-elements": "1.3.5",
    "@types/stripe-v3": "3.1.9",
    "babel-plugin-react-native-web": "0.14.9",
    "now": "21.0.1",
    "postinstall-prepare": "1.0.1",
    "prettier": "2.2.1",
    "react-app-rewired": "2.1.7",
    "react-refresh": "0.9.0",
    "serve": "11.2.0",
    "shx": "0.3.3",
    "typescript": "4.1.2",
    "webpack-bundle-analyzer": "3.5.2"
  },
  "peerDependencies": {
    "eslint": "*"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    "last 1 electron version",
    "last 1 chrome version",
    "last 1 firefox version",
    "last 1 safari version"
  ]
}