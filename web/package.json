{
  "name": "devhub-web",
  "version": "0.10.0",
  "private": true,
  "homepage": "https://brunolemos.github.io/devhub_ts",
  "scripts": {
    "build": "react-app-rewired build --scripts-version react-scripts-ts",
    "deploy": "gh-pages -d build",
    "eject": "react-app-rewired eject --scripts-version react-scripts-ts",
    "predeploy": "npm run build",
    "prepare": "patch-package",
    "serve": "serve build",
    "start": "react-app-rewired start --scripts-version react-scripts-ts",
    "test": "react-app-rewired test --scripts-version react-scripts-ts --env=jsdomreact-scripts-ts test --env=jsdom"
  },
  "dependencies": {
    "react": "^16.3.2",
    "react-art": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-native-web": "^0.6.0",
    "react-scripts-ts": "2.15.1"
  },
  "devDependencies": {
    "@types/jest": "^22.2.3",
    "@types/node": "^9.6.6",
    "@types/react": "^16.3.12",
    "@types/react-dom": "^16.0.5",
    "babel-plugin-react-native-web": "^0.6.0",
    "gh-pages": "^1.1.0",
    "patch-package": "^5.1.1",
    "postinstall-prepare": "^1.0.1",
    "react-app-rewired": "^1.5.0",
    "serve": "^6.5.5",
    "typescript": "^2.8.3"
  }
}
